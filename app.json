{
  "expo": {
    "name": "skincare",
    "slug": "skincare",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/cream.png",
      "resizeMode": "contain",
      "backgroundColor": "#AEBAC2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
